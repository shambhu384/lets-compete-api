{
  "required": [
    "@id",
    "competition",
    "participant",
    "@id",
    "@type",
    "accepte",
    "dateTime"
  ],
  "properties": {
    "@id": {
      "type": "string",
      "pattern": "^/"
    },
    "@type": {
      "constant": "InvitParticipation"
    },
    "accepte": {
      "type": ["boolean", "null"]
    },
    "dateTime": {
      "type": "string"
    },
    "competition": {
      "required": [
        "@id",
        "@type",
        "titre",
        "tempsDebut",
        "tempsFin"
      ],
      "properties": {
        "@id": {
          "type": "string"
        },
        "@type": {
          "constant": "Competition"
        },
        "titre": {
          "type": "string"
        },
        "tempsDebut": {
          "type": "string"
        },
        "tempsFin": {
          "type": "string"
        }
      }
    },
    "participant": {
      "properties": {
        "@id": {
          "type": "string"
        },
        "prenom": {
          "type": "string"
        },
        "nom": {
          "type": "string"
        },
        "email": {
          "type": "string",
          "pattern": ".*@.*\\..*"
        }
      }
    }
  }
}